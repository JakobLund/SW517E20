{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "type": {
            "type": "string"
        },
        "schemaLocation": {
            "type": "string"
        },
        "schemaVersion": {
            "type": "number"
        },
        "generator": {
            "type": "object",
            "properties": {
                "app": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                },
                "generatedAt": {
                    "type": "string"
                }
            },
            "required": [
                "app",
                "version",
                "generatedAt"
            ]
        },
        "content": {
            "type": "object",
            "properties": {
                "extractedFrom": {
                    "type": "string"
                },
                "confidence": {
                    "type": "string"
                },
                "publisher": {
                    "type": "string"
                },
                "publishedAt": {
                    "type": "string"
                },
                "publication": {
                    "type": "string"
                },
                "nmId": {
                    "type": "integer"
                },
                "page": {
                    "type": "integer"
                },
                "title": {
                    "type": "string"
                },
                "trompet": {
                    "type": "string"
                },
                "byline": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "email": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "name"
                    ]
                },
                "paragraphs": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "kind": {
                                    "type": "string"
                                },
                                "value": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "kind",
                                "value"
                            ]
                        },
                        
                    ]
                }
            },
            "required": [
                "extractedFrom",
                "publisher",
                "publishedAt",
                "publication",
                "page",
                "title",
                "paragraphs"
            ]
        }
    },
    "required": [
        "type",
        "schemaLocation",
        "schemaVersion",
        "generator",
        "content"
    ]
}
